import java.util.*;
public class Main
{
	public static void main(String[] args) {
		
		int a;
		Scanner sc=new Scanner (System.in);
		System.out.println("Enter number: ");
		a=sc.nextInt();
		if(a%2==0)
		{
		    System.out.println(+a+" is an even no.");
		    System.exit(0);
		}
		else
		{
		    System.out.println(+a+" is a odd no.");
		    System.exit(0);
		}
	}
}
