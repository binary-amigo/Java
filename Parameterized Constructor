import java.util.*;
class demo
{
    int a,b;
    demo(int x,int y)
    {
        a=x;
        b=y;
    }
    void printd()
    {
        System.out.println("a="+a);
        System.out.println("b="+b);
    }
}
public class Main{
    public static void main(String arg[])
    {
        demo d1=new demo(12,67);
        d1.printd();
    }
}
