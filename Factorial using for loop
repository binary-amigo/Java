import java.util.*;
public class Main
{
	public static void main(String[] args) {
		
		int num,fact=1;
		Scanner sc=new Scanner (System.in);
		System.out.println("Enter number: ");
		num=sc.nextInt();
		for(int i=1;i<=num;++i)
		{
		    fact=fact*i;
		}
		System.out.println("Factorial of "+num+" is "+fact);
		System.exit(0);
	}
}
