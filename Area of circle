import java.util.*;
public class Main
{
	public static void main(String[] args) {
		
		int r;
		float area;
		Scanner sc=new Scanner (System.in);
		System.out.println("Enter radius: ");
		r=sc.nextInt();
		area=3.14f*r*r;
		System.out.println("Area of circle with radius "+r+" is "+area);
		System.exit(0);
	}
}
